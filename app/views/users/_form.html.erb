<%= form_for @user do |f| %>
<div class="UserForm">
    <%= f.label "Email" %>
    <%= f.text_field :email %>
</div>
<div class="UserForm">
    <%= f.label "Username" %>
    <%= f.text_field :username %>
</div>
<div class="UserForm">
    <%= f.label "Password" %>
    <%= f.password_field :password %>
</div>
<div class="UserForm">
    <%= f.label "password_confirmation" %>
    <%= f.password_field :password_confirmation %>
</div>
<div class="field">
  <%= f.label :image %>
  <%= f.file_field :image %>
</div>
<div class="UserForm">
    <%= f.submit %>
</div>
<% end %>
