<h1> Sign up</h1>
<%= form_for @user do |f| %>
<div class="signup">
    <%= f.label "Email" %>
    <%= f.text_field :email %>
</div>
<div class="signup">
    <%= f.label "Username" %>
    <%= f.text_field :username %>
</div>
<div class="signup">
    <%= f.label "Password" %>
    <%= f.password_field :password %>
</div>
<div class="signup">
    <%= f.label "password_confirmation" %>
    <%= f.password_field :password_confirmation %>
</div>
<div class="signup">
    <%= f.submit "Sign Up" %>
</div>
<% end %>
